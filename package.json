{
  "name": "@epnsproject/backend-sdk-staging",
  "version": "2.0.3",
  "description": "SDK staging for sending notifications on EPNS",
  "scripts": {
    "git": "func() { git add . && git commit -m \"$1\" && git push origin HEAD; }; func",
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "rm -rf lib && tsc && mkdir lib/data && cp ./src/data/*.json ./lib/data/",
    "build:check": "tsc --noEmit",
    "format": "prettier --write \"src/**/*.ts\" \"src/**/*.js\"",
    "lint": "tslint -p tsconfig.json"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ethereum-push-notification-service/epns-backend-sdk-staging"
  },
  "files": [
    "lib"
  ],
  "publishConfig": {
    "access": "public"
  },
  "types": "./lib/index.d.ts",
  "main": "./lib/index.js",
  "module": "./lib/index.js",
  "author": "",
  "license": "ISC",
  "dependencies": {
    "axios": "^0.24.0",
    "crypto-js": "^4.1.1",
    "ethers": "^5.5.1",
    "ipfs-http-client": "^50.1.1",
    "moment": "^2.29.1",
    "uuid": "^8.3.2",
    "winston": "^3.3.3"
  },
  "devDependencies": {
    "@types/crypto-js": "^4.1.1",
    "@types/node": "^15.9.0",
    "@types/uuid": "^8.3.4",
    "prettier": "^2.3.0",
    "ts-node": "^10.0.0",
    "tslint": "^6.1.3",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^4.3.2"
  }
}
